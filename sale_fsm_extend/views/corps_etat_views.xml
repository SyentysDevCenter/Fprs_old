<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Form of corps etat -->
    <record id="corps_etat_view_form" model="ir.ui.view">
        <field name="name">Corps d'état</field>
        <field name="model">corps.etat</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <!-- Action of corps etat -->
    <record id="corps_etat_action" model="ir.actions.act_window">
        <field name="name">Corps d'état</field>
        <field name="res_model">corps.etat</field>
        <field name="view_mode">tree,form</field>
    </record>


</odoo>